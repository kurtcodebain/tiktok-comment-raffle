{
    "manifest_version": 3,
    "name": "Tanja's TikTok Raffle",
    "description": "Fetches all comments, picks a random winner, jumps to it and highlights it.",
    "version": "0.1",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["https://www.tiktok.com/*"],
            "js": ["finder.js"]
        }
    ]
}
